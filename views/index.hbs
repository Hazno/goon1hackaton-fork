<div class="container">
	<div class="row">
		<div class="menu col-xs-12 col-sm-12">
			<a-scene embedded arjs>
				<a-marker markerhandler emitevents="true" cursor="rayOrigin: mouse" id="familiebonus-marker" preset='custom' type='pattern' url='/markers/patt/familiebonus.patt'>
					<a-box id="box-model" position='0 0.5 0' material='color: red;' scale="1"></a-box>
				</a-marker>

                <a-marker preset="iphone8" emitevents="true" >
                    <a-entity
                            obj-model="obj: url(Obj/iphone8/iphone 8.obj); mtl: url(Obj/iphone8/iphone_8.mtl);"
                            scale="0.005 0.005 0.005" position='.5 0 1.3'>
                    </a-entity>
                </a-marker>

                <a-marker preset="familiebonus" emitevents="true">
                    <a-box position='0 0.5 0' material='color: blue;'></a-box>
                </a-marker>

                <a-marker preset="car" emitevents="true">
                    <a-entity
                            obj-model="obj: url(Obj/car/Shelby.obj); mtl: url(Obj/car/Shelby.mtl);"
                            scale="0.25 0.25 0.25" position='-0.1 0 0.5'>
                    </a-entity>
                </a-marker>

				<a-marker preset="logo" emitevents="true">
					<a-box position='-0.75 0 -0.75' material='color: blue;'></a-box>
					<a-box position='-0.75 0 0.75' material='color: green;'></a-box>
					<a-box position='0.75 0 -0.75' material='color: red;'></a-box>
					<a-box position='0.75 0 0.75' material='color: white;'></a-box>
				</a-marker>

				<a-entity camera></a-entity>
			</a-scene>
		</div>
	</div>
</div>
